{
    "url" : "http://pullbased/api/v1.0/master_package/{master_id}/",
    "dbfile" : "tests/db_files/set_master_status_zero.sql",
    "scenarios" :[
        {
            "method" : "delete",
            "name" : "set status of master 1 to 0",
            "url_params" :{
                "master_id" :"1"
            },
            "response_code": 200,
            "expected_content" : "tests/expected_reponses/template_store/master_template/master_package_deleted.json"
        },
        {
            "method" : "delete",
            "name" : "set status to 0 of already done master 1",
            "url_params" :{
                "master_id" :"1"
            },
            "response_code": 200,
            "expected_content" : "tests/expected_reponses/template_store/master_template/master_package_deleted.json"
        },
        {
            "method" : "delete",
            "name" : "Error case, set status 0 for non existant master id",
            "url_params" :{
                "master_id" :"7"
            },
            "response_code": 400,
            "expected_content" : "tests/expected_reponses/template_store/master_template/master_package_not_deleted.json"
        }
    ]
}