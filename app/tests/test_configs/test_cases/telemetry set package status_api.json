{
    "url": "http://pullbased/api/v1.0/edge_telemetry/{sub_route}/{edge_id}/{status}/",
    "scenarios": [
        {
            "method":"post",
            "name": "Package download 125216 success 0.05",
            "url_params": {
                "edge_id": "232425",
                "status": "success",
                "sub_route": "package_download"
            },
            "request_params": {
                "headers": {
                    "x-forwarded-for": "192.168.2.44"
                },
                "json":{
                    "edge_id": "125216",
                    "package_name": "125216.xz",
                    "template_set_version": "0.05"
                }
            },
            "response_code": 200,
            "expected_content": "tests/expected_reponses/telemetry/package_download_success.json"
        },
