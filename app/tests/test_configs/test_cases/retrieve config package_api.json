{
    "url" : "http://pullbased/api/v1.0/config_package/?{extra_params}",
    "dbfile" : "tests/db_files/retrieve_latest_template_set.sql",
    "scenarios" :[
        {
            "name" : "get config package for static default",
            "url_params" :{
                "extra_params" :"edge_id=232456"
            },
            "server_configs":{
                "edge-details":{
                    "template" : "accurate_responses/edge-details_static_default.json",
                    "jinja2" : {
                        "edge_type":"static",
                        "edge_sub_type":"default"
                    },
                    "return_code" : 200
                },
                "search":{
                    "methods":["POST"],
                    "template" : "accurate_responses/search_static_default.json",
                    "jinja2" : {
                        "edge_type":"static",
                        "edge_sub_type":"default"
                    },
                    "return_code" : 200
                }
            },
            "response_code": 200,
            "expected_content" : "tests/expected_reponses/config_store/retrieve_config_package_schema.json"
        },
        {
            "name" : "get config package for mobile c2cbus",
            "url_params" :{
                "extra_params" :"edge_id=232456"
            },
            "server_configs":{
                "edge-details":{
                    "template" : "accurate_responses/edge-details3.json",
                    "jinja2" : {
                        "edge_type":"mobile",
                        "edge_sub_type":"c2cbus"
                    },
                    "return_code" : 200
                },
                "search":{
                    "methods":["POST"],
                    "template" : "accurate_responses/search1.json",
                    "jinja2" : {
                        "edge_type":"mobile",
                        "edge_sub_type":"c2cbus"
                    },
                    "return_code" : 200
                }
            },
            "response_code": 200,
            "expected_content" : "tests/expected_reponses/config_store/retrieve_config_package_schema.json"
        },
        {
            "name" : "get config package for static default using mac address",
            "url_params" :{
                "extra_params" :"mac_address=23:24:25:26:27:28:29"
            },
            "server_configs":{
                "mac_to_edgeid":{
                    "template" : "accurate_responses/mac_to_edge1.json",
                    "jinja2" :{
                        "edge_id" : "232456"
                    },
                    "sub_route_names" :["mac_address"],
                    "return_code":200
                },
                "edge-details":{
                    "template" : "accurate_responses/edge-details_static_default.json",
                    "jinja2" : {
                        "edge_type":"static",
