                    },
                    "return_code" : 200
                },
                "search":{
                    "methods":["POST"],
                    "template" : "accurate_responses/search_static_default.json",
                    "jinja2" : {
                        "edge_type":"static",
                        "edge_sub_type":"default"
                    },
                    "return_code" : 200
                }
            },
            "response_code": 200,
            "expected_content" : "tests/expected_reponses/config_store/retrieve_config_package_schema.json"
        }
    ]
}