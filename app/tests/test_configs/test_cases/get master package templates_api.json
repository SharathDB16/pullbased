{
    "url" : "http://pullbased/api/v1.0/config_template/{master_id}/",
    "dbfile" : "tests/db_files/get_master_package_templates.sql",
    "scenarios" :[
        {
            "name" : "first master config templates list",
            "url_params" :{
                "master_id" :"1"
            },
            "response_code": 200,
            "expected_content" : "tests/expected_reponses/template_store/config_template/master_package_template_list1.json"
        },
        {
            "name" : "second master config templates list",
            "url_params" :{
                "master_id" :"2"
            },
            "response_code": 200,
            "expected_content" : "tests/expected_reponses/template_store/config_template/master_package_template_list2.json"
        },
        {
            "name" : "Error case, unknow master id",
            "url_params" :{
                "master_id" :"4"
            },
            "response_code": 404,
            "expected_content" : "tests/expected_reponses/template_store/config_template/master_id_not_found1.json"
        }
    ]
}