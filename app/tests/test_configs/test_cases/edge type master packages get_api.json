{
    "url" : "http://pullbased/api/v1.0/master_package/{edge_type}/{edge_sub_type}/",
    "dbfile" : "tests/db_files/get_edge_master_packages.sql",
    "scenarios" :[
        {
            "method" : "get",
            "name" : "master 1 and 2 for static, default",
            "url_params" :{
                "edge_type": "static",
                "edge_sub_type": "default"
            },
            "response_code": 200,
            "expected_content" : "tests/expected_reponses/template_store/master_template/static_default_master_packages.json"
        },
        {
            "method" : "get",
            "name" : "master 2 for mid, default",
            "url_params" :{
                "edge_type": "mid",
                "edge_sub_type": "default"
            },
            "response_code": 200,
            "expected_content" : "tests/expected_reponses/template_store/master_template/mid_default_master_packages.json"
        },
        {
            "method" : "get",
            "name" : "master 1 for mobile, c2cbus",
            "url_params" :{
                "edge_type": "mobile",
                "edge_sub_type": "c2cbus"
            },
            "response_code": 200,
            "expected_content" : "tests/expected_reponses/template_store/master_template/mobile_c2cbus_master_packages.json"
        },
        {
            "method" : "get",
            "name" : "nothing for mobile, default",
            "url_params" :{
                "edge_type": "mobile",
                "edge_sub_type": "default"
            },
            "response_code": 200,
            "expected_content" : "tests/expected_reponses/template_store/master_template/mobile_default_master_packages.json"
        },
        {
            "method" : "get",
            "name" : "Error case, unknow edge type",
            "url_params" :{
                "edge_type": "bile",
                "edge_sub_type": "bus"
            },
            "response_code": 404,
            "expected_content" : "tests/expected_reponses/template_store/master_template/bad_edge_type_master_packages.json"
        }
    ]
}